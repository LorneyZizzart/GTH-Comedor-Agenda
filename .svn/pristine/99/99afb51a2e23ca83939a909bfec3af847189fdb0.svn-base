package Controlador;

import Entidad.Cuestionario;
import Modelo.Ciclo_reporteModel;
import java.util.List;

public class Ciclo_reporteController {

    // Traemos todos los cuestionarios activos
    // el cuestionario tiene que estar en estado = 1
    public List<Cuestionario> GetAllCuestionarios(int ciclo_id) {
        Ciclo_reporteModel rep = new Ciclo_reporteModel();
        return rep.GetAllCuestionarios(ciclo_id);
    }

}
