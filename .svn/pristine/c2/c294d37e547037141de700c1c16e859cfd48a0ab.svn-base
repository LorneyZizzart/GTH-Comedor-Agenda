package Entidad;

/**
 *
 * @author Roberto
 */
public class Banco_respuestas {
    private int Banco_Respuestas_id;
    private String Opcion;
    private String Css;
    private String Seccion;
    private int Tipo_id;

    public int getBanco_Respuestas_id() {
        return Banco_Respuestas_id;
    }

    public void setBanco_Respuestas_id(int Banco_Respuestas_id) {
        this.Banco_Respuestas_id = Banco_Respuestas_id;
    }

    public String getOpcion() {
        return Opcion;
    }

    public void setOpcion(String Opcion) {
        this.Opcion = Opcion;
    }

    public String getCss() {
        return Css;
    }

    public void setCss(String Css) {
        this.Css = Css;
    }

    public String getSeccion() {
        return Seccion;
    }

    public void setSeccion(String Seccion) {
        this.Seccion = Seccion;
    }

    public int getTipo_id() {
        return Tipo_id;
    }

    public void setTipo_id(int Tipo_id) {
        this.Tipo_id = Tipo_id;
    }
    
}
